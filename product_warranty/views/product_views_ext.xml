<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_form_view_ext" model="ir.ui.view">
        <field name="name">product.template.form.ext</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <field name="categ_id" position="after">
               <field name="warranty_time"/>
            </field>
        </field>
    </record>

    <record id="product_variant_easy_edit_view_ext" model="ir.ui.view">
            <field name="name">product.product.view.form.easy.ext</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_variant_easy_edit_view"/>
            <field name="arch" type="xml">
            <field name="default_code" position="after">
               <field name="warranty_time"/>
            </field>
        </field>
    </record>

</odoo>
